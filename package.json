{
  "name": "eslint-plugin-wc",
  "version": "0.0.1",
  "description": "ESLint plugin for Web Components",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "docs/",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "build": "npm run lint && tsc",
    "build:watch": "tsc -w",
    "test": "mocha \"lib/test/**/*_test.js\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/43081j/eslint-plugin-wc.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "webcomponents",
    "wc"
  ],
  "author": "James Garbutt <https://github.com/43081j>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/43081j/eslint-plugin-wc/issues"
  },
  "homepage": "https://github.com/43081j/eslint-plugin-wc#readme",
  "dependencies": {
    "validate-element-name": "^2.1.1"
  },
  "peerDependencies": {
    "eslint": ">= 5"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/estree": "0.0.39",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "mocha": "^5.2.0",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3"
  }
}
